{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules"]
  },
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import [^,]+ from |^export | implements"
      }
    ]
  },
  "rulesDirectory": []
}
